clear 

parameters;


G_LAI  = @(x_pt,d) d*c1*x_pt./(c2 + x_pt);  
G_fpar = @(x) 1 - exp(-k*x);

F_par = @(x_pt,d) G_fpar(G_LAI(x_pt,d));

tspan = linspace(0,120);
clf

uR = 1e2;
%
Nd = 200;
initcont = linspace(1e-4,10,Nd);
color = jet(Nd);
i = 0;
for iint = initcont
    %
    i = i + 1;
    %
    [~,xt] = ode45(@(t,x) uR*F_par(x,d),tspan,iint);
    line(tspan,xt,'Color',color(i,:))
    %
end
%